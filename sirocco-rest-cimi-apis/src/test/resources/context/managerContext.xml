<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:jee="http://www.springframework.org/schema/jee"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
	http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee.xsd">


	<bean id="IJobManager" class="org.easymock.EasyMock"
		factory-method="createMock">
		<constructor-arg index="0"
			value="org.ow2.sirocco.cloudmanager.core.api.IJobManager" />
	</bean>
	<bean id="ICredentialsManager" class="org.easymock.EasyMock"
		factory-method="createMock">
		<constructor-arg index="0"
			value="org.ow2.sirocco.cloudmanager.core.api.ICredentialsManager" />
	</bean>
	<bean id="IMachineImageManager" class="org.easymock.EasyMock"
		factory-method="createMock">
		<constructor-arg index="0"
			value="org.ow2.sirocco.cloudmanager.core.api.IMachineImageManager" />
	</bean>
	<bean id="IMachineManager" class="org.easymock.EasyMock"
		factory-method="createMock">
		<constructor-arg index="0"
			value="org.ow2.sirocco.cloudmanager.core.api.IMachineManager" />
	</bean>
	<bean id="IVolumeManager" class="org.easymock.EasyMock"
		factory-method="createMock">
		<constructor-arg index="0"
			value="org.ow2.sirocco.cloudmanager.core.api.IVolumeManager" />
	</bean>


	<context:annotation-config />
	<context:component-scan base-package="org.ow2.sirocco.apis.rest.cimi" />
</beans>
